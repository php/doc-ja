<refentry xml:id="function.mysqli-send-query" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>mysqli_send_query</refname>
  <refname>mysqli->send_query</refname>
  <refpurpose>クエリを送信する</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <para>手続き型:</para>
  <methodsynopsis>
   <type>bool</type><methodname>mysqli_send_query</methodname>
   <methodparam><type>mysqli</type><parameter>link</parameter></methodparam>
   <methodparam><type>string</type><parameter>query</parameter></methodparam>
  </methodsynopsis>
  <para>オブジェクト指向型（メソッド）</para>
  <classsynopsis>
   <ooclass><classname>mysqli</classname></ooclass>
   <methodsynopsis>
    <type>bool</type><methodname>send_query</methodname>
    <methodparam><type>string</type><parameter>query</parameter></methodparam>
   </methodsynopsis>
  </classsynopsis>

  &warn.undocumented.func;
  &warn.deprecated.function.removed-5-3-0;

 </refsect1>

</refentry>

