<refentry xml:id="function.variant-abs" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>variant_abs</refname>
  <refpurpose>variant の絶対値を返す</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>variant</type><methodname>variant_abs</methodname>
   <methodparam><type>mixed</type><parameter>value</parameter></methodparam>
  </methodsynopsis>
  <para>
   variant の絶対値を返します。
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>value</parameter></term>
     <listitem>
      <para>
       variant。
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
   &com.variant-arith;
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   <parameter>value</parameter> の絶対値を返します。
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   失敗時に <classname>com_exception</classname> をスローします。
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>abs</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>
