<refentry xml:id="swfshape.drawglyph" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SWFShape::drawGlyph</refname>
  <refpurpose>
   指定したフォントのグリフ定義を使用して、
   指定した文字列の最初の文字を図形の中に描く
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>SWFShape::drawGlyph</methodname>
   <methodparam><type>SWFFont</type><parameter>font</parameter></methodparam>
   <methodparam><type>string</type><parameter>character</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>size</parameter></methodparam>
  </methodsynopsis>

  &warn.undocumented.func;

 </refsect1>
