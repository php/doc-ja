<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagickdraw.ellipse">
 <refnamediv>
  <refname>GmagickDraw::ellipse</refname>
  <refpurpose>楕円を画像上に描く</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>GmagickDraw</type><methodname>GmagickDraw::ellipse</methodname>
   <methodparam><type>float</type><parameter>ox</parameter></methodparam>
   <methodparam><type>float</type><parameter>oy</parameter></methodparam>
   <methodparam><type>float</type><parameter>rx</parameter></methodparam>
   <methodparam><type>float</type><parameter>ry</parameter></methodparam>
   <methodparam><type>float</type><parameter>start</parameter></methodparam>
   <methodparam><type>float</type><parameter>end</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   楕円を画像上に描きます。
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>ox</parameter></term>
    <listitem>
     <simpara>
      原点の x 座標。
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>oy</parameter></term>
    <listitem>
     <simpara>
      原点の y 座標。
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>rx</parameter></term>
    <listitem>
     <simpara>
      x 方向の半径。
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>ry</parameter></term>
    <listitem>
     <simpara>
      y 方向の半径。
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>start</parameter></term>
    <listitem>
     <simpara>
      開始角度。
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>end</parameter></term>
    <listitem>
     <simpara>
      終了角度。
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   <classname>GmagickDraw</classname> オブジェクトを返します。
  </simpara>
 </refsect1>


</refentry>
