<section id="mime-magic.configuration">
 &reftitle.runtime;
 &extension.runtime;
 <para>
    <table>
     <title>Mimetype 設定オプション</title>
  <tgroup cols="4">
   <thead>
    <row>
     <entry>名前</entry>
     <entry>デフォルト</entry>
     <entry>変更の可否</entry>
     <entry>変更履歴</entry>
    </row>
   </thead>
    <tbody>
     <row>
      <entry>mime_magic.debug</entry>
      <entry>"0"</entry>
      <entry>PHP_INI_SYSTEM</entry>
      <entry>PHP 5.0.0 以降で利用可能</entry>
     </row>
     <row>
      <entry>mime_magic.magicfile</entry>
      <entry>"/path/to/php/magic.mime"</entry>
      <entry>PHP_INI_SYSTEM</entry>
      <entry>PHP 4.3.0 以降で利用可能</entry>
     </row>
    </tbody>
   </tgroup>
  </table>
  &ini.php.constants;
 </para>

 &ini.descriptions.title;

 <para>
  <variablelist>
   <varlistentry id="ini.mime-magic.debug">
    <term>
     <parameter>mime_magic.debug</parameter>
     <type>bool</type>
    </term>
    <listitem>
     <para>
      デバッグを有効/無効にする
     </para>
    </listitem>
   </varlistentry>
   <varlistentry id="ini.mime-magic.magicfile">
    <term>
     <parameter>mime_magic.magicfile</parameter>
     <type>string</type>
    </term>
    <listitem>
     <para>
      magic.mime ファイルへのパス
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </para>
</section>

