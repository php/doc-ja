     <varname linkend="datetimeinterface.constants.rfc1123">DateTimeInterface::RFC1123</varname>
     <initializer>"D, d M Y H:i:s O"</initializer>
    </fieldsynopsis>
    <fieldsynopsis>
     <modifier>public</modifier>
     <modifier>const</modifier>
     <type>string</type>
