<appendix xml:id="sdo.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry>
   <term>
    <constant>SDO_DAS_ChangeSummary::NONE=0</constant>
      (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     変更の型 'none (変更していない)' を表します。
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SDO_DAS_ChangeSummary::MODIFICATION=1</constant>
     (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     変更の型 'modification (修正)' を表します。
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SDO_DAS_ChangeSummary::ADDITION=2</constant>
     (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     変更の型 'addition (追加)' を表します。
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>SDO_DAS_ChangeSummary::DELETION=3</constant>
     (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     変更の型 'deletion (削除)' を表します。
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
</appendix>

