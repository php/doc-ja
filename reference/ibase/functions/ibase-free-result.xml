<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.ibase-free-result">
 <refnamediv>
  <refname>ibase_free_result</refname>
  <refpurpose>結果セットを解放する</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ibase_free_result</methodname>
   <methodparam><type>resource</type><parameter>result_identifier</parameter></methodparam>
  </methodsynopsis>
  <para>
   結果セットを解放します。
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>result_identifier</parameter></term>
    <listitem>
     <para>
      <function>ibase_query</function> あるいは
      <function>ibase_execute</function>
      で作成した結果セット。
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>
