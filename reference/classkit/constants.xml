<appendix xml:id="classkit.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <para>
  <variablelist>
   <varlistentry xml:id="constant.classkit-acc-private">
    <term>
     <constant>CLASSKIT_ACC_PRIVATE</constant> 
     (<type>int</type>)
    </term>
    <listitem>
     <simpara>
      メソッドを <literal>private</literal> としてマークする
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.classkit-acc-protected">
    <term>
     <constant>CLASSKIT_ACC_PROTECTED</constant> 
     (<type>int</type>)
    </term>
    <listitem>
     <simpara>
      メソッドを <literal>protected</literal> としてマークする
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.classkit-acc-public">
    <term>
     <constant>CLASSKIT_ACC_PUBLIC</constant> 
     (<type>int</type>)
    </term>
    <listitem>
     <simpara>
      メソッドを <literal>public</literal> としてマークする
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </para>
</appendix>
