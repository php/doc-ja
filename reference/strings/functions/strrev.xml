<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.strrev">
 <refnamediv>
  <refname>strrev</refname>
  <refpurpose>文字列を逆順にする</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>strrev</methodname>
   <methodparam><type>string</type><parameter>string</parameter></methodparam>
  </methodsynopsis>
  <para>
   <parameter>string</parameter> を逆順にして返します。
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>string</parameter></term>
     <listitem>
      <para>
       逆順にしたい文字列。
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   逆順にした文字列を返します。
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>strrev</function> で文字列を逆順にする</title>
    <programlisting role="php">
<![CDATA[
<?php
echo strrev("Hello world!"); // 出力は "!dlrow olleH" となります
?>
]]>
    </programlisting>
   </example>
  </para>   
 </refsect1>

</refentry>

