<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-getimageresolution">
 <refnamediv>
  <refname>Imagick::getImageResolution</refname>
  <refpurpose>画像の X 方向と Y 方向の解像度を取得する</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>Imagick::getImageResolution</methodname>
   <void/>
  </methodsynopsis>
  <para>
   X 方向および Y 方向の解像度を取得します。
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   解像度を配列で返します。
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &imagick.imagick.throws;
  </para>
 </refsect1>

</refentry>

