<appendix xml:id="mongo.constants" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 &reftitle.constants;
 &extension.constants;

 <variablelist>
  <varlistentry>
   <term><constant>Mongo::DEFAULT_HOST</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     &quot;localhost&quot;
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>Mongo::DEFAULT_PORT</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     27017
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoDB::PROFILING_OFF</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     プロファイリングを無効にする。
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoDB::PROFILING_SLOW</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     遅い操作のみを追跡する。
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoDB::PROFILING_ON</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     すべての操作を追跡する。
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::LT</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     ～より小さい
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::LTE</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     ～以下
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::GT</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     ～より大きい
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::GTE</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     ～以上
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::IN</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     指定したセット内
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::NE</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     等しくない
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::ASC</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     昇順
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::DESC</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     降順
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_ARRAY</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     デフォルトのバイナリ型: バイト配列
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_UUID</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     全体で一意な ID
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_MD5</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     MD5 型
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_CUSTOM</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     ユーザ定義のバイナリ型
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>

</appendix>

